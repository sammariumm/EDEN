CREATE TABLE requests_new (
  id INTEGER PRIMARY KEY,
  user_id INTEGER NOT NULL,
  type TEXT CHECK(type IN ('job_listing', 'store')),
  status TEXT DEFAULT 'pending' CHECK(status IN ('pending', 'approved', 'rejected')),
  title TEXT NOT NULL,
  description TEXT NOT NULL,
  hourly_rate REAL,
  price REAL,
  subcategory TEXT CHECK(subcategory IN ('tools','decoration','plants','flowers','miscellaneous')),
  image TEXT,
  FOREIGN KEY (user_id) REFERENCES users(id)
);

-- Step 2: Copy existing data for matching columns (adjust column names if needed)
INSERT INTO requests_new (id, user_id, type, status, title, description, hourly_rate, price, subcategory, image)
SELECT id, user_id, type, status, title, description, hourly_rate, price, subcategory, image FROM requests;

-- Step 3: Drop old requests table
DROP TABLE requests;

-- Step 4: Rename new table to original name
ALTER TABLE requests_new RENAME TO requests;
